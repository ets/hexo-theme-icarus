<header id="header">
    <div id="header-main" class="header-inner">
        <div class="outer">
            <a href="<%- url_for() %>" id="logo">
                <%- (theme.customize.logo && theme.customize.logo.enabled ? '<i class="logo"></i>':'') %>
            </a>
            <nav id="main-nav">
                <% for (var i in theme.menu) { %>
                    <a class="main-nav-link" href="<%- url_for(theme.menu[i]) %>"><%= i %></a>
                <% } %>
            </nav>
            <% if(theme.customize.profile && theme.customize.profile.enabled) { %>
                <% var profile = theme.customize.profile; %>
                <nav id="sub-nav">
                    <div class="profile" id="profile-nav">
                        <a id="profile-anchor" href="javascript:;">
                            <img class="avatar" src="<%= ( profile.gravatar ? gravatar(profile.gravatar) : url_for(profile.avatar)) %>" />
                            <i class="fa fa-caret-down"></i>
                        </a>
                    </div>
                </nav>
            <% } %>
            <%- partial('search/index') %>
        </div>
        <div id="menus" class="outer">
          <div id="main-menu" class="dropdown dropdown-horizontal">
            <ul>
              <li class="webgen-menu-level1"><a href="https://www.catprep.com/index.html">Home</a></li>
              <li class="webgen-menu-level1"><a href="https://www.catprep.com/downloads.html">Practice Tests</a></li>
              <li class="webgen-menu-level1"><a href="https://www.catprep.com/news.html">Newsletter</a></li>
              <li class="webgen-menu-level1 webgen-menu-item-selected"><a style="padding:0" href="https://www.catprep.com/blog/"><span>Blog</span></a></li>
              <li class="webgen-menu-level1"><a href="https://www.catprep.com/educators.html">Educators</a></li>
              <li class="webgen-menu-level1"><a href="https://www.catprep.com/help.html">FAQ</a></li>
              <li class="webgen-menu-level1"><a href="https://www.catprep.com/contact.html">Contact Us</a></li>
            </ul>
          </div>
        </div>
    </div>

</header>
